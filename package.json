    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "jsdom": "^23.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
