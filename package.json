    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.26",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
